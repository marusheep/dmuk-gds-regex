CASE
    WHEN Campaign Type = "Deal" AND REGEXP_MATCH(Title without link,'.*独家.*') THEN "Dealmoon Exclusive"
    WHEN Campaign Type = "Deal" AND REGEXP_MATCH(Title without link,'.*晒货.*|.*晒单.*') THEN "Engagement Event"
    WHEN Campaign Type = "Deal" AND REGEXP_MATCH(Title without link,'.*赠.*|.*送.*礼.*|.*满额.*赠.*') THEN "GWP"
    WHEN Campaign Type = "Deal" AND REGEXP_MATCH(Title without link,'.*满.*减.*') THEN "Spend and Save"
    WHEN Campaign Type = "Deal" AND REGEXP_MATCH(Title without link,'.*上新.*|.*登陆.*|.*新品.*') THEN "New Launch"
    WHEN Campaign Type = "Deal" AND REGEXP_MATCH(Title without link,'.*推荐.*') THEN "Editorial"
    WHEN Campaign Type = "Deal" THEN "Direct Discount"
    ELSE ""
END
